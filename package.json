{
  "name": "@liquid-carrot/carrot",
  "version": "0.4.0",
  "description": "A Simple Node.js AI Library for Neural Network",
  "main": "dist/production/index.min.js",
  "types": "build/src/index.d.ts",
  "module": "dist/production/index.min.js",
  "unpkg": "dist/production/index.browser.min.js",
  "jsdelivr": "dist/production/index.browser.min.js",
  "browser": {
    "fs": false,
    "child_process": false
  },
  "scripts": {
    "deploy:src": "webpack",
    "deploy:docs": "typedoc --options typedoc.json",
    "pretest": "tsc --build --clean && tsc",
    "test": "mocha build/test/**/*",
    "test:ts": "mocha -r ts-node/register test/**/*",
    "posttest": "npm run check",
    "check": "npm run check:src && npm run check:test && npm run lint",
    "check:src": "prettier --check \"./src/**/*.ts\"",
    "check:test": "prettier --check \"./test/**/*.ts\"",
    "fix": "npm run fix:src && npm run fix:test",
    "fix:src": "prettier --write \"./src/**/*.ts\"",
    "fix:test": "prettier --write \"./test/**/*.ts\"",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c ./tslint.json 'src/**/*.ts' --fix",
    "prepare": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": "npm run fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liquidcarrot/carrot.git"
  },
  "keywords": [
    "Artificial Intelligence",
    "AI",
    "Deep Learning",
    "DL",
    "Machine Learning",
    "ML",
    "Neural Network",
    "Neural Networks",
    "NN",
    "Convolutional Neural Network",
    "Convolutional Neural Networks",
    "Convolutional NN",
    "Convolutional NNs",
    "CNN",
    "Artificial Neural Network",
    "Artificial Neural Networks",
    "Artificial NN",
    "Artificial NNs",
    "ANN",
    "Recurrent Neural Network",
    "Recurrent Neural Networks",
    "Recurrent NN",
    "Recurrent NNs",
    "RNN",
    "Perceptron",
    "Sigmoid",
    "TanH",
    "ArcTan"
  ],
  "author": "Liquid Carrot <people@liquidcarrot.io> (https://liquidcarrot.io)",
  "contributors": [
    "Luis Ernesto Carbonell <luis@liquidcarrot.io>",
    "Christian George Echevarria <chris@liquidcarrot.io>"
  ],
  "files": [
    "/src",
    "/test",
    "/dist",
    "/types"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liquidcarrot/carrot/issues"
  },
  "homepage": "https://liquidcarrot.io/",
  "dependencies": {
    "activations": "^1.1.2",
    "chai": "^4.2.0",
    "mnist": "^1.1.0",
    "mocha": "^8.1.1",
    "timsort": "^0.3.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/expect": "^24.3.0",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/timsort": "^0.3.0",
    "awesome-typescript-loader": "^5.2.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.20",
    "prettier": "2.0.5",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "webpack-cli": "^3.3.12"
  }
}
